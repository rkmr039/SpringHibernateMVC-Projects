package com.hcl.streams;

import java.util.ArrayList;
import java.util.List;

public class EmployMain {

	public static void main(String[] args) {
		List<Employ> list = new ArrayList<Employ>();
		list.add(new Employ(10,"Apple",1010f,'E'));
		list.add(new Employ(10,"Banana",1010f,'M'));
		list.add(new Employ(10,"Carrot",1010f,'M'));
		list.add(new Employ(10,"Dates",1010f,'C'));
		list.add(new Employ(10,"Apricot",1010f,'M'));
	}
}
