package com.hcl.streams;

import java.util.ArrayList;
import java.util.List;

public class EmployMain {

	public static void main(String[] args) {
		List<Employ> list = new ArrayList<Employ>();
		list.add(new Employ(10,"Apple",1010f,'E'));
		list.add(new Employ(10,"Banana",2020f,'M'));
		list.add(new Employ(10,"Carrot",3030f,'M'));
		list.add(new Employ(10,"Dates",4040f,'C'));
		list.add(new Employ(10,"Apricot",5050f,'M'));
	}
}
